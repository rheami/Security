from bs4 import BeautifulSoup
from bs4 import SoupStrainer
import webbrowser

soup = BeautifulSoup(html_doc, 'html.parser')

code0 = soup.findall(class="nopadding classcell0")
code1 = soup.find(class="nopadding classcell1")
code2 = soup.find(class="nopadding classcell2")
code3 = soup.find(class="nopadding classcell3")
code4 = soup.find(class="nopadding classcell4")

if code1||code2 :
	file = open("vulnerabilite1.html","w")
	print(code1.next_sibling)
	print(code2.next_sibling)
if code3||code4:
	file = open("vulnerabilite2.html","w")
	print(code3.next_sibling)
	print(code4.next_sibling)
else:
	file = open("vulnerabilite0.html","w")
	print(code0.next_sibling)

